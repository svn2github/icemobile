    <tag>
        <description>Geolocation Tag</description>
        <display-name>ICEmobile Geolocation Tag</display-name>
        <name>geolocation</name>
        <tag-class>org.icemobile.jsp.tags.GeolocationTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Pagewide unique identifier</description>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of submitted value,
                will be a String of the form lat,lon,alt,dir
            </description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Determines if control is disabled</description>
            <name>disabled</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>If true, javascript call resolves to watchPosition,
                else
                uses getCurrentPosition for a one time update
            </description>
            <name>continuousUpdates</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Valid values are true, false, and asNeeded. Defaults to
                asNeeded. Indicates
                whether or not to turn on the GPS hardware. Some devices require
                this setting to
                fetch any reading at all
            </description>
            <name>enableHighPrecision</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>Define the max age (in seconds) of an acceptable cached
                Position reading.
            </description>
            <name>maximumAge</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description>Defines the maximum time allowed for a successful
                update (in seconds)
            </description>
            <name>timeout</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
    </tag>

