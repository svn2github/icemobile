    <tag>
        <description>Device detection and script/css insertion.</description>
        <display-name>ICEmobile Device Detection</display-name>
        <name>deviceResource</name>
        <tag-class>org.icemobile.jsp.tags.DeviceResourceTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Theme to use.
            </description>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>If true (default), the deviceResource tag will cause
                IOS 6 to
                render a Smart App Banner for the application, allowing the user
                to install ICEmobile SX,
                if it is not already installed, and open it, if the app is
                installed.
                If the user chooses to open the installed app, the session will
                be auto-registered.
            </description>
            <name>includeIOSSmartAppBanner</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>If set to true (default false) the ICEpush script will
                be rendered, allowing
                the application to receive Ajax Push notifications, and use the
                ICEpush client-side API.
            </description>
            <name>includePush</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <description>Geolocation Tag.</description>
        <display-name>ICEmobile Geolocation Tag</display-name>
        <name>geolocation</name>
        <tag-class>org.icemobile.jsp.tags.GeolocationTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Pagewide unique identifier. Required for locating hidden
                         field by id.</description>
            <name>id</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of submitted value,
                will be a String of the form lat,lon,alt,dir
            </description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Determines if control is disabled.</description>
            <name>disabled</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>If true, javascript call resolves to watchPosition,
                else
                uses getCurrentPosition for a one time update.
            </description>
            <name>continuousUpdates</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Valid values are true, false, and asNeeded. Defaults to
                asNeeded. Indicates
                whether or not to turn on the GPS hardware. Some devices require
                this setting to
                fetch any reading at all.
            </description>
            <name>enableHighPrecision</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>Define the max age (in seconds) of an acceptable cached
                Position reading.
            </description>
            <name>maximumAge</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description>Defines the maximum time allowed for a successful
                update (in seconds).
            </description>
            <name>timeout</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
    </tag>

    <tag>
        <description>Input Text field.</description>
        <display-name>Input Text Tag</display-name>
        <name>inputText</name>
        <tag-class>org.icemobile.jsp.tags.InputTextTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Pagewide unique identifier.</description>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of submitted value.</description>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>If type="inputtext", text field is rendered as such, otherwise
            textfield is rendered.</description>
            <name>type</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Determines if control is disabled.</description>
            <name>disabled</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Determines if control is read only.</description>
            <name>readOnly</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>User defined CSS class.</description>
            <name>styleClass</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inline style definition.</description>
            <name>style</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>The (optional) initial value of the field.</description>
            <name>value</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>A step value for number values.
            </description>
            <name>step</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>A placeholder string appearing in the textfield.
            </description>
            <name>placeholder</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Hint to container about spelling auto correction. Valid
                values are on/off.
            </description>
            <name>autoCorrect</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>Hint to container about spelling auto completion. Valid
                values are on/off.
            </description>
            <name>autoComplete</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Hint to container about spelling auto capitalization.
                Valid values are on/off.
            </description>
            <name>autoCapitalize</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Row count for input text area.</description>
            <name>rows</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Column count for input text area.</description>
            <name>cols</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>The label to render for the input field.</description>
            <name>label</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

