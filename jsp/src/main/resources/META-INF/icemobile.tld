<taglib xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
        version="2.1">

    <description>ICEmobile JSP Tag Library.</description>
    <display-name>ICEmobile JSP Tag Library</display-name>
    <tlib-version>2.0.0</tlib-version>
    <short-name>icemobile</short-name>
    <uri>http://www.icemobile.org/tags</uri>
    
    <listener>
        <listener-class>org.icemobile.jsp.servlet.ICEmobileServletContextListener</listener-class>
    </listener>
    
     <listener>
        <listener-class>org.icemobile.jsp.servlet.MobiClientListener</listener-class>
    </listener>
    

    <tag>
        <description>Show augmented reality video overlay.</description>
        <display-name>ICEmobile Augmented Reality</display-name>
        <name>augmentedReality</name>
        <tag-class>org.icemobile.jsp.tags.AugTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>Pagewide unique identifier.</description>
            <name>id</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inline style definition.</description>
            <name>style</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>User defined CSS class.</description>
            <name>styleClass</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>low-level URL-encoded label=lat,lon,alt,dir,url
                location list (deprecated)
            </description>
            <name>params</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Determines if control is disabled.</description>
            <name>disabled</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>The value of the label for the button. default Value is
                'Augmented Reality'.
            </description>
            <name>buttonLabel</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Declare an augmented reality location.</description>
        <display-name>ICEmobile Augmented Reality Location</display-name>
        <name>augmentedRealityLocation</name>
        <tag-class>org.icemobile.jsp.tags.AugmentedRealityLocationTag
        </tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Location text label.</description>
            <name>locationLabel</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Location latitude.</description>
            <name>locationLat</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Location longitude.</description>
            <name>locationLon</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Location altitude.</description>
            <name>locationAlt</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Location compass direction 0-359.</description>
            <name>locationDir</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Location icon URL.</description>
            <name>locationIcon</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Declare an augmented reality marker.</description>
